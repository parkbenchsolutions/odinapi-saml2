{
    "name": "parkbenchsolutions/odinapi-saml2",
    "description": "A Laravel package for Saml2 integration as a SP (service provider), based on OneLogin toolkit.",
    "keywords": [
        "laravel",
        "saml",
        "saml2",
        "onelogin"
    ],
    "homepage": "https://github.com/parkbenchsolutions/odinapi-saml2",
    "license": "copywrite Park Bench Solutions Inc",
    "version": "0.1.1",
    "authors": [
        {
            "name": "Park Bench Solutions",
            "email": "support@parkbenchsolutions.com"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "ext-openssl": "*",
        "illuminate/support": ">=5.0.0",
        "parkbenchsolutions/odinapi-onelogin": ">=0.1"
    },
    "require-dev": {
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~4.0"
    },
    "autoload": {
        "psr-0": {
            "Parkbenchsolutions\\OdinapiSaml2\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Parkbenchsolutions\\OdinapiSaml2\\Saml2ServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev"
}
